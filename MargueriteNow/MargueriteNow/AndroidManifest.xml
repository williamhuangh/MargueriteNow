<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hacktech.stanford.margueritenow"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service
            android:enabled="true"
            android:name=".DownloadTask"
            android:label="Download Task" >
        </service>
        <activity
            android:name="hacktech.stanford.margueritenow.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplayStops"
            android:label="@string/title_activity_display_stops" >
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplayLine"
            android:label="@string/title_activity_display_line" >
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplayLineY"
            android:label="@string/title_activity_display_line_y" >
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplayTime"
            android:label="@string/title_activity_display_time" >
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplayLineX"
            android:label="@string/title_activity_display_line_x" >
        </activity>
        <activity
            android:name="hacktech.stanford.margueritenow.DisplaySLAC"
            android:label="@string/title_activity_display_slac" >
        </activity>
    </application>

</manifest>
